<script>
$(document).ready(function(){
  // Open mobile menu
  $("button[aria-label='Open main menu']").on("click", function(){
    $(".lg\\:hidden[role='dialog']").show();
  });

  // Close mobile menu
  $("button[aria-label='Close menu']").on("click", function(){
    $(".lg\\:hidden[role='dialog']").hide();
  });
});
